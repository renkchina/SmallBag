<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="rxpicker_black">#000</color>
    <color name="rxpicker_colorAccent">#FF4081</color>
    <color name="rxpicker_colorPrimary">#000000</color>
    <color name="rxpicker_colorPrimaryAlpha">#44000000</color>
    <color name="rxpicker_colorPrimaryDark">#ff212121</color>
    <color name="rxpicker_width">#FFF</color>
    <string name="all_phone_album">全部相册</string>
