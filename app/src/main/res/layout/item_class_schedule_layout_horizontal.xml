<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="37dp"
    android:background="@color/data_month_gray"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/class_schedule_top_date_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:includeFontPadding="false"
        android:maxEms="2"
        android:maxWidth="37dp"
        android:maxLines="2"
        android:text="11"
        android:textSize="12sp" />

    <View
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/background_gray" />

    <TextView
        android:id="@+id/class_schedule_one_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:lineSpacingExtra="0dp"
        android:maxEms="2"
        android:maxLines="2"
        android:text="1"
        android:textSize="14sp" />

    <View
        android:id="@+id/one_line"
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/background_gray" />

    <TextView
        android:id="@+id/class_schedule_two_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:lineSpacingExtra="0dp"
        android:maxEms="2"
        android:maxLines="2"
        android:text="2"
        android:textSize="14sp" />

    <View
        android:id="@+id/two_line"
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/background_gray" />

    <TextView
        android:id="@+id/class_schedule_three_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:lineSpacingExtra="0dp"
        android:maxEms="2"
        android:maxLines="2"
        android:text="3"
        android:textSize="14sp" />

    <View
        android:id="@+id/three_line"
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/background_gray" />

    <TextView
        android:id="@+id/class_schedule_four_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:lineSpacingExtra="0dp"
        android:maxEms="2"
        android:maxLines="2"
        android:text="4"
        android:textSize="14sp" />

    <View
        android:id="@+id/four_line"
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/background_gray" />

    <TextView
        android:id="@+id/class_schedule_rest_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:lineSpacingExtra="0dp"
        android:maxEms="2"
        android:maxLines="2"
        android:text="5"
        android:textSize="14sp" />

    <View
        android:id="@+id/five_line"
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/background_gray" />

    <TextView
        android:id="@+id/class_schedule_five_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:lineSpacingExtra="0dp"
        android:maxEms="2"
        android:maxLines="2"
        android:text="6"
        android:textSize="14sp" />

    <View
        android:id="@+id/sex_line"
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/background_gray" />

    <TextView
        android:id="@+id/class_schedule_six_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:gravity="center"
        android:lineSpacingExtra="0dp"
        android:maxEms="2"
        android:maxLines="2"
        android:text="7"
        android:textSize="14sp" />
    <View
        android:id="@+id/seven_line"
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/background_gray" />
</LinearLayout>
